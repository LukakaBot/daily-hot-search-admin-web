{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "nest build",
  "functions": {
    "dist/src/main.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "HEAD",
        "OPTIONS"
      ]
    }
  ]
}